{
  "name": "cnn-cipher",
  "version": "0.1.0",
  "description": "A graphql query constructor.",
  "directories": {
    "lib": "./lib",
    "test": "./test"
  },
  "scripts": {
    "build": "grunt",
    "generate-authors": "tools/generate-authors.sh",
    "generate-changelog": "changelog-maker --group",
    "generate-docs": "rm -rf docs && esdoc -c .esdoc.json",
    "test": "eslint . && test/validate-json-files.sh",
    "test": "grunt default",
    "test-jscs": "grunt jscs",
    "test-jshint": "grunt jshint",
    "update-apply": "ncu -u",
    "update-check": "ncu"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cnnlabs/cnn-cipher.git"
  },
  "bugs": {
    "url": "https://github.com/cnnlabs/cnn-cipher/issues"
  },
  "homepage": "https://github.com/cnnlabs/cnn-cipher#readme",
  "dependencies": {
    "bower": "1.8.2"
  },
  "devDependencies": {
    "casperjs": "1.1.3",
    "changelog-maker": "2.2.6",
    "esdoc": "1.0.3",
    "eslint": "4.7.1",
    "grunt": "1.0.1",
    "grunt-casper": "0.4.2",
    "grunt-contrib-copy": "1.0.0",
    "grunt-contrib-jshint": "1.1.0",
    "grunt-contrib-uglify": "3.1.0",
    "grunt-jscs": "3.0.1",
    "grunt-jsonlint": "1.1.0",
    "jscs": "3.0.7",
    "jscs-stylish": "0.3.1",
    "jsdoc": "3.5.5",
    "jshint": "2.9.5",
    "jshint-stylish": "2.2.1",
    "load-grunt-tasks": "3.5.2",
    "npm-check-updates": "2.12.1",
    "phantomjs-prebuilt": "2.1.15"
  },
  "keywords": [
    "cnn",
    "cnnlabs",
    "graphql"
  ],
  "license": "MIT"
}
